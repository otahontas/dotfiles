[push]
    default = matching
[alias] 
    ap = add -p
    aa = add -A
    a = add
    s = status
    d = diff
    co = checkout
    br = branch
    last = log -1 HEAD
    cm = commit -m
    cane = commit --amend --no-edit
    cedit = commit --amend
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
