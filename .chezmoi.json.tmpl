{
  "data": {
    "spotifyd": {
      "username": {{ output "op" "item" "get" "Spotify" "--fields" "label=username"
                     | trim
                     | quote
                  }},
      "computer_name": {{ output "scutil" "--get" "ComputerName" | trim | quote}}
    },
    "git": {
      "email": {{ promptStringOnce .git "email" "Git email" | quote }},
      "signingkey": {{ promptStringOnce .git "signingkey" "Git signing key" | quote }}
    },
    "zsh": {
      "integrations": {{ default list (get .zsh "integrations") | toJson }}
    }
  },
  "diff": {
    "format": "git",
    "pager": "diff-so-fancy"
  }
}
